{
  "name": "portfolio",
  "version": "1.1.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "metalsmith": "DEBUG=metalsmith-layouts metalsmith",
    "optimize-images": "lepto -c lepto.config.json",
    "build": "yarn metalsmith && yarn prod && yarn optimize-images"
  },
  "author": "Riku Laajala",
  "license": "ISC",
  "dependencies": {
    "postcss-nested": "^4.2.1",
    "tailwindcss": "^1.1.4"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^6.0.3",
    "jstransformer-handlebars": "^1.1.0",
    "jstransformer-nunjucks": "^1.0.0",
    "laravel-mix": "^5.0.1",
    "laravel-mix-purgecss": "^4.2.0",
    "lepto": "^1.2.0",
    "lepto-resize": "^1.0.1",
    "lepto-vibrant-color": "^1.0.0",
    "metalsmith": "^2.3.0",
    "metalsmith-asciidoc": "^1.1.0",
    "metalsmith-asciidoctor": "^1.1.2",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-drafts": "^1.0.0",
    "metalsmith-layouts": "^2.3.1",
    "metalsmith-markdown": "^1.3.0",
    "vue-template-compiler": "^2.6.11"
  }
}
