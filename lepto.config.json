{
  "input": "src",
  "output": "dist",
  "watch": true,
  "openGUI": true,
  "dataOutput": "lepto.config.json",
  "filters": [
    {
      "glob": [
        "**/*.jpg",
        "**/*.jpeg"
      ],
      "use": [
        {
          "name": "lepto-resize",
          "maxWidth": 1200
        },
        "lepto-vibrant-color",
        {
          "name": "lepto.jpeg",
          "quality": 70
        }
      ]
    },
    {
      "glob": "**/*.png",
      "use": [
        {
          "name": "lepto-resize",
          "retina": [
            2
          ]
        },
        {
          "name": "lepto.png",
          "quality": "70-80"
        }
      ]
    }
